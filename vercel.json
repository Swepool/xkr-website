{
  "redirects": [
    {
      "source": "/en/faucet",
      "destination": "https://faucet.kryptokrona.org"
    },
    {
      "source": "/sv/faucet",
      "destination": "https://faucet.kryptokrona.org"
    },
    {
      "source": "/no/faucet",
      "destination": "https://faucet.kryptokrona.org"
    },
    {
      "source": "/stats/js/script.js",
      "destination": "https://plausible.io/js/script.js"
    },
    {
      "source": "/stats/api/event",
      "destination": "https://plausible.io/api/event"
    }
  ],
  "rewrites": [
    {
      "source": "/stats/js/script.js",
      "destination": "https://plausible.io/js/script.js"
    },
    {
      "source": "/stats/api/event",
      "destination": "https://plausible.io/api/event"
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "sdk.kryptokrona.org"
        }
      ],
      "destination": "https://kryptokrona.github.io/kryptokrona-kotlin-sdk/:path*"
    }
  ]
}

